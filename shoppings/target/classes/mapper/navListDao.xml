<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.shoppings.dao.INavLiatDao">
    <select id="selectNavList" resultType="com.shoppings.model.NavList" parameterType="Integer">
           SELECT navList.id,title,src FROM navList INNER JOIN tb_navList ON tb_id=navlist_id
    </select>

    <insert id="navInsert" parameterType="com.shoppings.model.NavList" >
            INSERT INTO navList (navlist_id,title,src) VALUES(#{id},#{title},#{src});

    </insert>
</mapper>